<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Brasserie-front</display-name>
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude></include-prelude>
    </jsp-property-group>
  </jsp-config>
  <servlet>
    <description></description>
    <display-name>ProduitsController</display-name>
    <servlet-name>ProduitsController</servlet-name>
    <servlet-class>controllers.ProduitsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProduitsController</servlet-name>
    <url-pattern>/ProduitsController</url-pattern>
  </servlet-mapping>
</web-app>